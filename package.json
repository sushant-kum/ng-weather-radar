{
  "name": "ng-weather-radar",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "version": "npm run tool:changelog && git add CHANGELOG.md package.json package-lock.json",
    "start": "npm run serve::app",
    "ng": "ng",
    "build": "npm run build:prod::app",
    "test": "ng test",
    "preserve::app": "npm run tool:version-info-gen -- --dev",
    "serve::app": "ng serve --port 5010",
    "prebuild:prod::app": "npm run tool:version-info-gen",
    "build:prod::app": "ng build",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier *.{css,html,js,json,md,scss,ts}",
    "format:check": "prettier --check ./**/*.{css,html,js,json,md,scss,ts}",
    "format:fix": "prettier --write ./**/*.{css,html,js,json,md,scss,ts}",
    "tool:commit": "cz",
    "tool:set-env": "node tools/set-env/set-env.tool.ts",
    "tool:version-info-gen": "node tools/version-info-gen/version-info-gen.tool.ts",
    "tool:changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "pretool:scully": "npm run build",
    "tool:scully": "npx scully --project=ng-weather-radar --scanRoutes",
    "pretool:scully::serve": "npm run tool:scully",
    "tool:scully::serve": "npx scully serve --project=ng-weather-radar"
  },
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/material": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@scullyio/init": "^2.1.41",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.0",
    "dotenv": "^16.3.1",
    "replace-in-file": "^7.0.1",
    "rxjs": "^6.6.0",
    "tslib": "^2.6.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jasmine": "^4.3.5",
    "@types/node": "^18.17.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "detect-secrets": "^1.0.6",
    "eslint": "^8.47.0",
    "eslint-config-ngx": "^1.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "^5.1.0",
    "karma": "^6.3.6",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^14.0.0",
    "ng-lint-staged": "^12.0.4",
    "prettier": "^3.0.2",
    "scully-plugin-minify-html": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
